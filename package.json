{
  "name": "webpack-study",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.prod.js",
  "scripts": {
    "test": "jest",
    "dev": "cross-env NODE_ENV=development alfred-cli-service serve",
    "build": "cross-env NODE_ENV=production alfred-cli-service build",
    "build:analyze": "cross-env NODE_ENV=production alfred-cli-service build --analyze",
    "lint:style": "stylelint --fix 'src/**/*.{css,less}'",
    "lint": "eslint -v && eslint --fix --ext .js,.jsx,.tx,.tsx src/",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "check-types": "tsc",
    "inspect": "alfred-cli-service inspect"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.less": [
      "stylelint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "android >= 4.4.4",
    "ios >= 8"
  ],
  "devDependencies": {
    "@types/react": "^16.9.56",
    "alfred-babel-preset": "^1.0.0",
    "alfred-cli-service": "^1.0.7",
    "antd": "^4.7.2",
    "autoprefixer": "^9.8.6",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.4.2",
    "prettier": "^2.2.1",
    "react-refresh": "^0.14.0",
    "stylelint": "^13.8.0",
    "typescript": "^4.0.5",
    "alfred-fabric": "^2.0.3"
  },
  "dependencies": {
    "core-js": "^3.34.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0"
  }
}