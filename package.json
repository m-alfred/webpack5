{
  "name": "webpack-study",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.prod.js",
  "scripts": {
    "test": "jest",
    "dev": "cross-env NODE_ENV=development node ./scripts/dev-server.js",
    "build": "cross-env NODE_ENV=production node ./scripts/build-prod.js",
    "build:trace": "cross-env NODE_ENV=production node --trace-deprecation ./scripts/build-prod.js",
    "build:analyze": "yarn build --analyze",
    "lint:style": "stylelint --fix 'src/**/*.{css,less}'",
    "lint": "eslint -v && eslint --fix --ext .js,.jsx,.tx,.tsx src/",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "check-types": "tsc",
    "inspect": "node ./scripts/inspect.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.less": [
      "stylelint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "antd": "^4.7.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^10.4.2",
    "mini-css-extract-plugin": "^1.0.0",
    "minimist": "^1.2.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.4.31",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "string.prototype.padstart": "^3.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^4.47.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-chain": "^6.5.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.14.0",
    "core-js": "3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0"
  }
}